<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:controller="sample.PetrolController" xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
