   <Label text="Prawdopodobienstwo utraty klienta:" GridPane.rowIndex="3" GridPane.columnIndex="0" />
   <Label fx:id="prawdopodobienstwo2" GridPane.rowIndex="3" GridPane.columnIndex="1" />

